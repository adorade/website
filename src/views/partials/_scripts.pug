//- partials/_scripts.pug
block external-js
  //--- Adorade JS Files, external scripts here (cdn)
  script#vendors(defer src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous")
  script#cc-script(defer src="https://cdn.jsdelivr.net/npm/vanilla-cookieconsent@2.9.2/dist/cookieconsent.min.js"
    integrity="sha384-8y31riJCK5o7EJKT3uqBIXLua6tM6FC0Q06Zl0yrrd/TsiJVX/v78t9Rb5c5335W" crossorigin="anonymous")

block vendors-js
  //--- Vendors main JS

block custom-js
  //--- Main Script
  if entry.inline
    script#main-min-script(defer src="js/script.min.js")
  else
    script#main-dev-script(defer src="js/script-dev.js")
