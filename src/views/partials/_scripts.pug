//- partials/_scripts.pug
block external-js
  //--- Adorade JS Files
  //- external scripts here (cdn), then ...

block vendors-js
  //--- Vendors main JS
  //- jQuery 3.5.1, popper.js 1.16.1, unikorn 1.1.1
  //- yall.js 3.2.0, browser.js 4.0.0, cookieconsent 3.1.1
  script#vendors(defer src="js/vendors.min.js")

block custom-js
  //--- Main Script
  if entry.inline
    script#main-min-script(defer src="js/script.min.js")
  else
    script#main-dev-script(defer src="js/script-dev.js")
